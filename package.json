{
  "name": "mock-server",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "author": "lincenying <lincenying@qq.com>",
  "scripts": {
    "prepare": "husky install",
    "serve": "vite",
    "build_test": "vite build --mode test --emptyOutDir",
    "build_pre": "vite build --mode staging --emptyOutDir",
    "build_prod": "vite build --emptyOutDir",
    "start": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.tsx --fix"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "vue": "^3.2.47",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@lincy/eslint-config": "^1.1.2",
    "@lincy/stylelint-config": "^1.15.10",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.1.2",
    "@unocss/reset": "^0.51.12",
    "@vitejs/plugin-vue": "^4.2.1",
    "default-passive-events": "^2.0.0",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mockjs": "^1.1.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "unocss": "^0.51.12",
    "unplugin-vue-macros": "^2.1.4",
    "url-search-params-polyfill": "^8.1.1",
    "vite": "^4.3.5",
    "vite-plugin-mock": "^3.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "postcss",
        "terser",
        "prettier",
        "esbuild"
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  }
}
